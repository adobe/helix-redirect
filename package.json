{
  "name": "@adobe/helix-redirect",
  "version": "1.1.51",
  "private": true,
  "description": "Helix Redirect",
  "main": "src/index.js",
  "scripts": {
    "test": " nyc --reporter=text --reporter=lcov --check-coverage --branches 100 --statements 100 --lines 100 mocha  -i -g 'Post-Deploy'",
    "test-ci": "nyc --reporter=text --reporter=lcov --check-coverage --branches 100 --statements 100 --lines 100 mocha  -i -g 'Post-Deploy' --reporter xunit --reporter-options output=./junit/test-results.xml && codecov",
    "test-postdeploy": "mocha --reporter xunit --reporter-options output=./junit/test-results.xml -g 'Post-Deploy'",
    "lint": "./node_modules/.bin/eslint .",
    "semantic-release": "semantic-release",
    "docs": "npx jsdoc2md -c .jsdoc.json --files 'src/*.js'  > docs/API.md",
    "commit": "git-cz",
    "build": "hedy -v",
    "deploy": "hedy -v --deploy --test=/_status_check/healthcheck.json",
    "deploy-sequences": "hedy --no-build -no-hints -l latest -l major -l minor",
    "deploy-ci": "hedy -v --deploy --test=/_status_check/healthcheck.json --pkgVersion=ci$CIRCLE_BUILD_NUM -l ci"
  },
  "wsk": {
    "name": "helix-services/redirect@${version}"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adobe/helix-redirect"
  },
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/adobe/helix-redirect/issues"
  },
  "homepage": "https://github.com/adobe/helix-redirect#readme",
  "dependencies": {
    "@adobe/helix-shared": "7.19.4",
    "@adobe/helix-status": "9.0.0",
    "@adobe/openwhisk-action-logger": "2.3.1",
    "@adobe/openwhisk-action-utils": "4.4.0",
    "node-fetch": "2.6.1"
  },
  "devDependencies": {
    "@adobe/eslint-config-helix": "1.1.3",
    "@adobe/helix-deploy": "1.18.2",
    "@pollyjs/adapter-node-http": "5.1.0",
    "@pollyjs/core": "5.1.0",
    "@pollyjs/persister-fs": "5.0.0",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/exec": "5.0.0",
    "@semantic-release/git": "9.0.0",
    "chai": "4.2.0",
    "chai-http": "4.3.0",
    "codecov": "3.8.1",
    "commitizen": "4.2.3",
    "cz-conventional-changelog": "3.3.0",
    "dotenv": "8.2.0",
    "eslint": "7.18.0",
    "eslint-plugin-header": "3.1.0",
    "eslint-plugin-import": "2.22.1",
    "jsdoc-to-markdown": "6.0.1",
    "junit-report-builder": "2.1.0",
    "lint-staged": "10.5.3",
    "mocha": "8.2.1",
    "nyc": "15.1.0",
    "proxyquire": "2.1.3",
    "semantic-release": "17.3.7"
  },
  "lint-staged": {
    "*.js": "eslint"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npx lint-staged"
    }
  }
}
